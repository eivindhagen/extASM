; --------------------------------------------
; Multiple single elements
; --------------------------------------------

VLD1.8   {D1},[R2]
VLD1.16  {D1},[R2]
VLD1.32  {D1},[R2]
VLD1.64  {D1},[R2]

VLD1.8   {D1},[R2@64]
VLD1.8   {D1,D2},[R3@128]
VLD1.8   {D1,D2,D3,D4},[R5@128]
VLD1.8   {D1,D2,D3,D4},[R5@256]

VLD1.8   {D1},[R2]!
VLD1.8   {D1},[R2],R3

VLD1.8   {D1,D2},[R3]
VLD1.8   {D1,D2,D3},[R4]
VLD1.8   {D1,D2,D3,D4},[R5]

; --------------------------------------------
; Single element to a lane
; --------------------------------------------

VLD1.8   {D1[1]},[R2]
VLD1.16  {D1[1]},[R2]
VLD1.32  {D1[1]},[R2]

VLD1.16  {D1[1]},[R2@16]
VLD1.32  {D1[1]},[R2@32]

VLD1.8   {D1[1]},[R2]!
VLD1.8   {D1[1]},[R2],R3

; --------------------------------------------
; Single element to all lanes
; --------------------------------------------

VLD1.8   {D1[]},[R2]
VLD1.16  {D1[]},[R2]
VLD1.32  {D1[]},[R2]

VLD1.16  {D1[]},[R2@16]
VLD1.32  {D1[]},[R2@32]

VLD1.8   {D1[]},[R2]!
VLD1.8   {D1[]},[R2],R3

VLD1.8   {D1[],D2[]},[R3]

; --------------------------------------------
; Multiple single elements
; --------------------------------------------

VST1.8   {D1},[R2]
VST1.16  {D1},[R2]
VST1.32  {D1},[R2]
VST1.64  {D1},[R2]

VST1.8   {D1},[R2@64]
VST1.8   {D1,D2},[R3@128]
VST1.8   {D1,D2,D3,D4},[R5@128]
VST1.8   {D1,D2,D3,D4},[R5@256]

VST1.8   {D1},[R2]!
VST1.8   {D1},[R2],R3

VST1.8   {D1,D2},[R3]
VST1.8   {D1,D2,D3},[R4]
VST1.8   {D1,D2,D3,D4},[R5]

; --------------------------------------------
; Single element from a lane
; --------------------------------------------

VST1.8   {D1[1]},[R2]
VST1.16  {D1[1]},[R2]
VST1.32  {D1[1]},[R2]

VST1.16  {D1[1]},[R2@16]
VST1.32  {D1[1]},[R2@32]

VST1.8   {D1[1]},[R2]!
